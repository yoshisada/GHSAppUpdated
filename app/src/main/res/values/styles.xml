        <item name="colorPrimary">@color/MainLayout</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textSize">30sp</item>
        <item name="logo">@drawable/whitesmall2</item>
        <item name="bar">@color/MainLayout</item>
        <item name="padding">30dp</item>
